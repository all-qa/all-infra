GH.tpl.epic.renderEpicPalette = function(opt_data, opt_ignored) {
  var output = '<ul class="ghx-swatches">';
  var colorList200 = opt_data.colors;
  var colorListLen200 = colorList200.length;
  for (var colorIndex200 = 0; colorIndex200 < colorListLen200; colorIndex200++) {
    var colorData200 = colorList200[colorIndex200];
    output += '<li class="ghx-swatch js-change-epic-color ' + soy.$$escapeHtml(colorData200) + '" data-color-id="' + soy.$$escapeHtml(colorData200) + '">e</li>';
  }
  output += '</ul>';
  return output;
};
